// Isolated Wang Syntax Tests - One feature at a time
// Run each section independently to identify exact failures

// ========== TEST SET 1: Basic Arrows ==========
console.log("=== ARROW FUNCTIONS ===")

// Test 1.1: Simplest arrow
let arrow1 = x => x
console.log("1.1 Simple arrow defined")

// Test 1.2: Arrow with parentheses
let arrow2 = (x) => x
console.log("1.2 Paren arrow defined")

// Test 1.3: Two params arrow
let arrow3 = (a, b) => a + b
console.log("1.3 Two param arrow defined")

// Test 1.4: Arrow with block
let arrow4 = (x) => {
    return x * 2
}
console.log("1.4 Block arrow defined")

// Test 1.5: Call the arrows
console.log("1.1 result: " + arrow1(5))
console.log("1.2 result: " + arrow2(10))
console.log("1.3 result: " + arrow3(2, 3))
console.log("1.4 result: " + arrow4(7))